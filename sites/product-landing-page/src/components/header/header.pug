include ../logo/logo

mixin header(props = {})
	- const {  } = props;

	.header
		.header__wrapper.wrapper
			+logo
			nav.header__nav
				a.prevdef.header__nav-link(href="#") Home
				a.prevdef.header__nav-link(href="#") Product
				a.prevdef.header__nav-link(href="#") About
				a.prevdef.header__nav-link(href="#") Contact
			.header__sign-up-n-hamburger-container
				a.prevdef.header__sign-up-link(href="#") Sign Up
				.header__hamburger
					.header__hamburger-elem.header__hamburger-elem1
					.header__hamburger-elem.header__hamburger-elem2
					.header__hamburger-elem.header__hamburger-elem3
