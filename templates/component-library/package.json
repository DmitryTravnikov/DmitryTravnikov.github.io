{
  "name": "webpack-project",
  "version": "1.0.0",
  "description": "Шаблон проекта для быстрого старта с webpack, pug, scss и jQuery",
  "private": true,
  "scripts": {
    "start": "cross-env-shell NODE_ENV=development webpack-dev-server --config-register @babel/register --open --config \"./webpack/config/webpack.dev.js\"",
    "build": "cross-env-shell NODE_ENV=production webpack --config-register @babel/register --config \"./webpack/config/webpack.prod.js\"",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist",
    "serve": "serve \"./dist\" -s"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [
    "html",
    "css",
    "js",
    "webpack",
    "pug",
    "scss",
    "jQuery"
  ],
  "author": "Dmitry Travnikov",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "app-root-path": "^3.0.0",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.1",
    "cssnano": "^4.1.10",
    "dotenv-webpack": "^7.0.3",
    "file-loader": "^6.0.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.3.0",
    "image-webpack-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.10.0",
    "postcss-loader": "^3.0.0",
    "pug": "2.0.4",
    "pug-loader": "^2.4.0",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.3",
    "serve": "^11.3.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.1"
  },
  "dependencies": {
    "@google/markerclustererplus": "^5.1.3",
    "@googlemaps/js-api-loader": "^1.12.8",
    "@popperjs/core": "^2.10.2",
    "animate.css": "^4.1.1",
    "bootstrap": "^5.1.3",
    "fullpage.js": "2.9.7",
    "inputmask": "^5.0.6",
    "jquery": "^3.5.1",
    "masonry-layout": "^4.2.2",
    "normalize.css": "^8.0.1",
    "owl.carousel": "^2.3.4",
    "page-scroll-to-id": "^1.6.8",
    "smooth-scrollbar": "^8.7.0",
    "wow.js": "^1.2.2"
  }
}
